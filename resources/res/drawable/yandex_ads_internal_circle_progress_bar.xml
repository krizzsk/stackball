<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270" android:toDegrees="270">
            <shape android:shape="ring" android:useLevel="true">
                <corners android:radius="6dp"/>
                <rotate android:fromDegrees="0" android:toDegrees="360"/>
                <solid android:color="@color/yandex_ads_internal_text_color_white"/>
            </shape>
        </rotate>
    </item>
</layer-list>
